{
    "name": "javaabu/jaravel",
    "description": "Customized version of Laravel for Javaabu",
    "homepage": "https://github.com/Javaabu/jaravel",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "jaravel"
    ],
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.10",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Jaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Jaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "12.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
